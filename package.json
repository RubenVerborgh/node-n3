{
  "name": "n3",
  "version": "1.0.0-beta.2",
  "description": "Lightning fast, asynchronous, streaming Turtle / N3 / RDF library.",
  "author": "Ruben Verborgh <ruben.verborgh@gmail.com>",
  "keywords": [
    "turtle",
    "rdf",
    "n3",
    "streaming",
    "asynchronous"
  ],
  "license": "MIT",
  "main": "./N3.js",
  "engines": {
    "node": ">=4.0"
  },
  "files": [
    "N3.js",
    "lib"
  ],
  "devDependencies": {
    "async": "^2.0.1",
    "chai": "^4.0.2",
    "chai-things": "^0.2.0",
    "colors": "^1.1.2",
    "coveralls": "^3.0.0",
    "docco": "^0.8.0",
    "eslint": "^5.5.0",
    "follow-redirects": "^1.4.1",
    "mocha": "^5.0.1",
    "nyc": "^13.0.1",
    "pre-commit": "^1.2.2"
  },
  "scripts": {
    "test": "nyc mocha",
    "lint": "eslint lib perf test spec",
    "coveralls": "nyc --reporter=text-lcov mocha | coveralls",
    "spec": "node spec/turtle-spec && node spec/trig-spec && node spec/ntriples-spec && node spec/nquads-spec",
    "spec-clean": "rm -r spec/turtle spec/trig",
    "docs": "docco lib/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdfjs/N3.js.git"
  },
  "bugs": {
    "url": "https://github.com/rdfjs/N3.js/issues"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
